{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/ui": "^4.0.0",
    "@nuxtjs/supabase": "^2.0.0",
    "@supabase/supabase-js": "^2.58.0",
    "nuxt": "^4.1.2",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nuxt/eslint": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "typescript": "^5.9.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{ts,vue,tsx}": [
      "bash -c 'vue-tsc --noEmit'"
    ]
  }
}
